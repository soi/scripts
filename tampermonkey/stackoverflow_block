function toggle_visibility_id(element) {
		let el = document.getElementById(element);
		if (el) {
			el.style.display = 'none';
		}
}

function toggle_visibility_class(element) {
	let elements = document.getElementsByClassName(element);
	let n = elements.length;
	if(n > 0) {
		for (var i = 0; i < n; i++) {
			elements[i].style.display = 'none';
		}
	}
}


toggle_visibility_class('community-bulletin', false);
toggle_visibility_class('everyonelovesstackoverflow', false);
toggle_visibility_class('jobs', false);
toggle_visibility_id('hireme', true);
toggle_visibility_id('hot-network-questions', true);
toggle_visibility_id('sidebar', true);
toggle_visibility_class('mb16', false);
toggle_visibility_class('user-gravatar32', false);
toggle_visibility_class('-flair', false);
toggle_visibility_id('left-sidebar', true);
toggle_visibility_class('ml12', false);
