function toggle_visibility_id(element) {
		let el = document.getElementById(element);
		if (el) {
			el.style.display = 'none';
		}
}

function toggle_visibility_class(element) {
	let elements = document.getElementsByClassName(element);
	let n = elements.length;
	if(n > 0) {
		for (var i = 0; i < n; i++) {
			elements[i].style.display = 'none';
		}
	}
}


toggle_visibility_class('community-bulletin');
toggle_visibility_class('everyonelovesstackoverflow');
toggle_visibility_class('jobs');
toggle_visibility_id('hireme');
toggle_visibility_id('hot-network-questions');
toggle_visibility_id('sidebar');
toggle_visibility_class('mb16');
toggle_visibility_class('user-gravatar32');
toggle_visibility_class('-flair');
toggle_visibility_id('left-sidebar');
toggle_visibility_class('ml12');
toggle_visibility_class('top-bar');
